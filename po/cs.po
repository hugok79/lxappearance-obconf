#: ../src/main.c:71 ../src/main.c:73 ../src/main.c:75
#, c-format
msgid "Copyright (c)"
msgstr "Copyright (c)"

#: ../src/main.c:86
#, c-format
msgid "Syntax: obconf [options] [ARCHIVE.obt]\n"
msgstr "Syntaxe: obconf [přepínače] [ARCHIV.obt]\n"

#: ../src/main.c:87
#, c-format
msgid ""
"\n"
"Options:\n"
msgstr ""
"\n"
"Přepínače:\n"

#: ../src/main.c:88
#, c-format
msgid "  --help                Display this help and exit\n"
msgstr "  --help                Zobrazit tuto nápovědu a skončit\n"

#: ../src/main.c:89
#, c-format
msgid "  --version             Display the version and exit\n"
msgstr "  --version             Zobrazit verzi a skončit\n"

#: ../src/main.c:90
#, c-format
msgid "  --install ARCHIVE.obt Install the given theme archive and select it\n"
msgstr "  --install ARCHIV.obt  Instalovat daný archiv a vybrat ho\n"

#: ../src/main.c:91
#, c-format
msgid ""
"  --archive THEME       Create a theme archive from the given theme "
"directory\n"
msgstr "  --archive MOTIV       Vytvořit archiv z daného motivu\n"

#: ../src/main.c:92
#, c-format
msgid "  --config-file FILE    Specify the path to the config file to use\n"
msgstr ""

