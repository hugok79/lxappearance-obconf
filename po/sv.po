#: ../src/main.c:86
#, c-format
msgid "Syntax: obconf [options] [ARCHIVE.obt]\n"
msgstr "Syntax: obconf [alternativ] [ARKIV.obt]\n"

#: ../src/main.c:87
#, c-format
msgid ""
"\n"
"Options:\n"
msgstr ""
"\n"
"Alternativ:\n"

#: ../src/main.c:88
#, c-format
msgid "  --help                Display this help and exit\n"
msgstr "  --help                Visa den här hjälpen och avsluta\n"

#: ../src/main.c:89
#, c-format
msgid "  --version             Display the version and exit\n"
msgstr "  --version             Visa version och avsluta\n"

#: ../src/main.c:90
#, c-format
msgid "  --install ARCHIVE.obt Install the given theme archive and select it\n"
msgstr ""
"  --install ARKIV.obt   Installera det givna temaarkivet och välj det\n"

#: ../src/main.c:91
#, c-format
msgid ""
"  --archive THEME       Create a theme archive from the given theme "
"directory\n"
msgstr ""
"  --archive TEMA        Skapa ett temaarkiv från den givna temakatalogen\n"

#: ../src/main.c:92
#, c-format
msgid "  --config-file FILE    Specify the path to the config file to use\n"
msgstr ""

